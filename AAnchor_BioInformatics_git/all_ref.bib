issn = {15455963},
journal = {IEEE/ACM Transactions on Computational Biology and Bioinformatics},
keywords = {Structural bioinformatics,intermediate resolution electron density maps,refined classification,secondary structure identification,spherical harmonic descriptors},
number = {1},
pages = {228--239},
title = {{RENNSH: A novel $\alpha$-helix identification approach for intermediate resolution electron density maps}},
volume = {9},
