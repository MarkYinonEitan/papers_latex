@article{Raucci2017,
abstract = {Cells are interactive living systems where protein-protein bindings constitute fundamental keystones in cell construction, regulation and signalling. How physico-chemical and geometrical properties of the protein interfaces associate their effects to determine a functional interaction remains far from fully understood. Different models estimating the strength of the interaction between proteins forming a complex have been previously proposed and recently compared against experimental measures. Here, we explore the geometry of contact distribution at protein-protein interfaces and provide an improved model of the binding energy between pairs of proteins forming complexes. Local Interaction Signal Analysis (LISA) is a radial function based on terms describing favorable and non-favorable contacts obtained by Density Functional Theory, the Support-Core-Rim interface residue distribution, non-interacting charged residues and secondary structures contribution. The three-dimensional organisation of the contacts and their contribu-tion on localised hot-sites over the entire interaction surface were numerically evaluated. LISA achieves a correlation of 0.81 (and RMSE of 2.35 ± 0.38 kcal/mol) when tested on 125 complexes for which experi-mental measurements were realised with selected technologies (stopped-flow, surface plasmon resonance, spectroscopy and isothermal titration calorimetry). LISA's performance is stable for subsets defined by functional composition and extent of conformational changes upon complex formation. A large-scale comparison with 17 other functions was assessed and demonstrated the power of the geometrical model in the understanding of complex binding.},
author = {Raucci, Raffaele and Laine, Elodie and Carbone, Alessandra},
file = {::},
keywords = {Density Functional Theory,atom-atom contact,binding affinity,electron density,favorable contact,non-covalent interaction,non-favorable contact,non-interacting surface,protein-protein interaction,protein-protein interface,reduced density gradient},
title = {{Structure Local Interaction Signal Analysis predicts protein-protein binding affinity Local Interaction Signal Analysis predicts protein-protein binding affinity}},
year = {2017}
}
@article{Terwilliger2018,
abstract = {Recent advances in the field of electron cryomicroscopy (cryo-EM) have resulted in a rapidly increasing number of atomic models of biomacromolecules that have been solved using this technique and deposited in the Protein Data Bank and the Electron Microscopy Data Bank. Similar to macromolecular crystallography, validation tools for these models and maps are required. While some of these validation tools may be borrowed from crystallography, new methods specifically designed for cryo-EM validation are required. Here, new computational methods and tools implemented in PHENIX are discussed, including d99 to estimate resolution, phenix.auto{\_}sharpen to improve maps and phenix.mtriage to analyze cryo-EM maps. It is suggested that cryo-EM half-maps and masks should be deposited to facilitate the evaluation and validation of cryo-EM-derived atomic models and maps. The application of these tools to deposited cryo-EM atomic models and maps is also presented.},
author = {Terwilliger, Thomas C and Adams, D and Urzhumtsev, Alexandre},
doi = {10.1107/S2059798318009324},
keywords = {atomic models,cryo-em,data quality,field of electron cryomicroscopy,have,increasing number of atomic,model,models of biomacromolecules,quality,recent advances in the,resolution,resulted in a rapidly,validation},
pages = {814--840},
publisher = {International Union of Crystallography},
title = {{New tools for the analysis and validation of cryo-EM maps and atomic models research papers}},
year = {2018}
}
@article{Ashkenazy2010,
abstract = {It is informative to detect highly conserved positions in proteins and nucleic acid sequence/structure since they are often indicative of structural and/or functional importance. ConSurf (http://consurf.tau.ac.il) and ConSeq (http://conseq.tau.ac.il) are two well-established web servers for calculating the evolutionary conservation of amino acid positions in proteins using an empirical Bayesian inference, starting from protein structure and sequence, respectively. Here, we present the new version of the ConSurf web server that combines the two independent servers, providing an easier and more intuitive step-by-step interface, while offering the user more flexibility during the process. In addition, the new version of ConSurf calculates the evolutionary rates for nucleic acid sequences. The new version is freely available at: http://consurf.tau.ac.il/.},
author = {Ashkenazy, Haim and Erez, Elana and Martz, Eric and Pupko, Tal and Ben-Tal, Nir},
doi = {10.1093/nar/gkq399},
file = {:Users/markroza/Library/Application Support/Mendeley Desktop/Downloaded/Ashkenazy et al. - 2010 - ConSurf 2010 Calculating evolutionary conservation in sequence and structure of proteins and nucleic acids.pdf:pdf},
isbn = {0305-1048},
issn = {03051048},
journal = {Nucleic Acids Research},
pmid = {20478830},
title = {{ConSurf 2010: Calculating evolutionary conservation in sequence and structure of proteins and nucleic acids}},
year = {2010}
}
