author = {Vreven, Thom and Moal, Iain H. and Vangone, Anna and Pierce, Brian G. and Kastritis, Panagiotis L. and Torchala, Mieczyslaw and Chaleil, Raphael and Jim{\'{e}}nez-Garc{\'{i}}a, Brian and Bates, Paul A. and Fernandez-Recio, Juan and Bonvin, Alexandre M.J.J. and Weng, Zhiping},
