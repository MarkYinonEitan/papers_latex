keywords = {LIBG Crystallography and complementary methods,electron cryo-microscopy reconstructions,model building,refinement},
pmid = {25615868},
title = {{Tools for macromolecular model building and refinement into electron cryo-microscopy reconstructions}},
year = {2015}
}
@techreport{Chothial1986,
abstract = {Homologous proteins have regions which retain the same general fold and regions where the folds differ. For pairs of distantly related proteins (residue identity-20{\%}), the regions with the same fold may comprise less than half of each molecule. The regions with the same general fold differ in structure by amounts that increase as the amino acid sequences diverge. The root mean square deviation in the positions of the main chain atoms, A, is related to the fraction of mutated residues, H, by the expression: A(A) = 0.40 el87H.},
author = {Chothial, Cyrus and Lesk2, Arthur M and Perutz, M F},
booktitle = {The EMBO Journal},
file = {::},
